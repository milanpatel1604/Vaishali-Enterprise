{
  "name": "vaishali-enterprise",
  "version": "1.0.0",
  "description": "Vaishali Enterprise - Premium Laminate Company Website",
  "main": "server/index.js",
  "scripts": {
    "client": "npm start --prefix client",
    "server": "nodemon server/index.js",
    "dev": "concurrently \"npm run server\" \"npm run client\"",
    "build": "cd client && npm install && npm run build",
    "install-all": "npm install && cd client && npm install",
    "start": "NODE_ENV=production node server/index.js"
  },
  "keywords": ["laminates", "enterprise", "react", "nodejs"],
  "author": "Vaishali Enterprise",
  "license": "ISC",
  "dependencies": {
    "express": "^4.18.2",
    "mongoose": "^8.0.3",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "body-parser": "^1.20.2",
    "nodemailer": "^6.9.7"
  },
  "devDependencies": {
    "nodemon": "^3.0.2",
    "concurrently": "^8.2.2"
  }
}
